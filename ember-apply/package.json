{
  "name": "ember-apply",
  "version": "2.7.0",
  "description": "automatic integration and configuration from the community",
  "repository": {
    "url": "https://github.com/NullVoxPopuli/ember-apply",
    "type": "https",
    "directory": "packages/ember-apply"
  },
  "main": "src/index.js",
  "exports": {
    ".": {
      "import": "./src/index.js",
      "types": "./declarations/src/index.d.ts"
    },
    "./test-utils": "./src/test-utils.js"
  },
  "type": "module",
  "author": "NullVoxPopuli",
  "license": "MIT",
  "private": false,
  "bin": {
    "ember-apply": "src/cli/index.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./declarations/src/*",
        "./declarations/src/index.d.ts"
      ]
    }
  },
  "types": "./declarations/src/",
  "files": [
    "src/**/*",
    "declarations/**/*",
    "README.md"
  ],
  "scripts": {
    "prepack": "pnpm build && cp ../README.md ./",
    "build": "tsc --build",
    "lint:types": "tsc",
    "lint": "pnpm lint:js",
    "lint:js": "eslint .",
    "lint:fix": "pnpm lint:js --fix",
    "test": "vitest --coverage --no-watch --no-threads",
    "test:watch": "vitest --watch --no-threads"
  },
  "dependencies": {
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.8",
    "chalk": "^5.2.0",
    "ember-template-recast": "^6.1.4",
    "execa": "^7.1.1",
    "find-up": "^6.3.0",
    "fs-extra": "^11.1.1",
    "jscodeshift": "^0.15.0",
    "latest-version": "^7.0.0",
    "ora": "^6.3.1",
    "pacote": "^15.2.0",
    "postcss": "^8.4.24",
    "posthtml": "^0.16.5",
    "posthtml-boolean-attributes": "^0.3.1",
    "semver": "^7.5.1",
    "sort-object-keys": "^1.1.3",
    "unified": "^10.1.1",
    "yargs": "^17.7.2",
    "yarn-workspaces-list": "^0.2.0"
  },
  "devDependencies": {
    "@nullvoxpopuli/eslint-configs": "^3.1.3",
    "@types/fs-extra": "^11.0.0",
    "@types/jscodeshift": "^0.11.3",
    "@types/node": "^18.0.0",
    "@types/pacote": "^11.1.3",
    "@types/semver": "^7.3.9",
    "@types/sort-object-keys": "^1.1.0",
    "@types/yargs": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "@vitest/ui": "^0.28.0",
    "babel-eslint": "^10.1.0",
    "c8": "^7.11.0",
    "eslint": "^8.42.0",
    "eslint-plugin-decorator-position": "^5.0.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.8.8",
    "typescript": "4.8.4",
    "vite": "^4.3.9",
    "vitest": "^0.28.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "volta": {
    "node": "18.14.1",
    "yarn": "1.22.19"
  }
}
