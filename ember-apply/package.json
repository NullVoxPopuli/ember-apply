{
  "name": "ember-apply",
  "version": "2.2.1",
  "description": "automatic integration and configuration from the community",
  "repository": {
    "url": "https://github.com/NullVoxPopuli/ember-apply",
    "type": "https",
    "directory": "packages/ember-apply"
  },
  "main": "src/index.js",
  "exports": {
    ".": "./src/index.js",
    "./test-utils": "./src/test-utils.js"
  },
  "type": "module",
  "author": "NullVoxPopuli",
  "license": "MIT",
  "private": false,
  "bin": {
    "ember-apply": "src/cli/index.js"
  },
  "types": "./declarations/src/",
  "files": [
    "src/**/*",
    "declarations/**/*",
    "README.md"
  ],
  "scripts": {
    "prepack": "yarn build:types && cp ../README.md ./",
    "build:types": "tsc --build",
    "lint": "yarn lint:js",
    "lint:js": "eslint .",
    "lint:fix": "yarn lint:js --fix",
    "test": "vitest --coverage --no-watch --no-threads",
    "test:watch": "vitest --watch --no-threads"
  },
  "dependencies": {
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.8",
    "chalk": "^5.0.0",
    "ember-template-recast": "^6.1.2",
    "execa": "^6.0.0",
    "fs-extra": "^10.0.0",
    "jscodeshift": "^0.13.1",
    "latest-version": "^6.0.0",
    "ora": "^6.0.1",
    "pacote": "^13.0.0",
    "posthtml": "^0.16.5",
    "semver": "^7.3.5",
    "unified": "^10.1.1",
    "yargs": "^17.3.1",
    "yarn-workspaces-list": "^0.2.0"
  },
  "devDependencies": {
    "@nullvoxpopuli/eslint-configs": "^2.1.17",
    "@types/fs-extra": "^9.0.13",
    "@types/jscodeshift": "^0.11.3",
    "@types/node": "^17.0.8",
    "@types/pacote": "^11.1.3",
    "@types/semver": "^7.3.9",
    "@types/yargs": "^17.0.8",
    "@vitest/ui": "^0.6.0",
    "babel-eslint": "^10.1.0",
    "c8": "^7.11.0",
    "eslint": "^7.0.0",
    "eslint-plugin-decorator-position": "^4.0.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.1",
    "semantic-release": "^19.0.0",
    "semantic-release-monorepo": "^7.0.0",
    "typescript": "4.6.2",
    "vite": "^2.7.13",
    "vitest": "^0.5.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "volta": {
    "node": "16.14.0",
    "yarn": "1.22.17"
  }
