{
  "name": "@ember-apply/tailwind",
  "description": "ember-apply plugin for tailwind",
  "version": "1.0.1",
  "type": "module",
  "license": "MIT",
  "preset": "index.js",
  "author": "NullVoxPopuli",
  "files": [
    "index.js",
    "files/**/*"
  ],
  "scripts": {
    "publish": "semantic-release -e semantic-release-monorepo",
    "publish:dry-run": "yarn publish --dry-run",
    "build:types": "tsc --build",
    "lint": "yarn lint:js",
    "lint:js": "eslint .",
    "lint:fix": "yarn lint:js --fix",
    "test": "vitest --coverage --no-watch",
    "test:watch": "vitest --watch"
  },
  "dependencies": {
    "ember-apply": "*"
  },
  "devDependencies": {
    "@nullvoxpopuli/eslint-configs": "^2.1.17",
    "@semantic-release/changelog": "^6.0.0",
    "@semantic-release/git": "^10.0.0",
    "autoprefixer": "^10.0.0",
    "babel-eslint": "^10.1.0",
    "c8": "^7.11.0",
    "eslint": "^7.0.0",
    "eslint-plugin-decorator-position": "^4.0.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "postcss": "^8.0.0",
    "prettier": "^2.5.1",
    "semantic-release": "^18.0.0",
    "semantic-release-monorepo": "^7.0.0",
    "tailwindcss": "^3.0.0",
    "typescript": "^4.5.4",
    "vitest": "^0.1.0",
    "vite": "^2.7.10"
  },
  "release": {
    "extends": "semantic-release-monorepo",
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/github",
      "@semantic-release/git"
    ]
  }
}
